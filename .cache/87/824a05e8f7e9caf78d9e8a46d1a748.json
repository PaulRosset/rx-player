nction castToObservable<T>(value? : any) : Observable<T> {\n  if (value instanceof Observable) {\n    return value;\n  }\n\n  if (value && typeof value.subscribe === \"function\") {\n    const valObsLike = value as IObservableLike<T>;\n    return new Observable((obs) => {\n      const sub = valObsLike.subscribe((val : T)   => { obs.next(val); },\n                                       (err : unknown) => { obs.error(err); },\n                                       () => { obs.complete(); });\n      return () => {\n        if (sub && sub.dispose) {\n          sub.dispose();\n        }\n        else if (sub && sub.unsubscribe) {\n          sub.unsubscribe();\n        }\n      };\n    });\n  }\n\n  if (value && typeof value.then === \"function\") {\n    return observableFrom(value as Promise<T>);\n  }\n\n  return observableOf(value);\n}\n\nexport default castToObservable;\n"},"lineCount":null}},"error":null,"hash":"ae58fe3943c0725b1de3dfcb1f36e458","cacheData":{"env":{}}}